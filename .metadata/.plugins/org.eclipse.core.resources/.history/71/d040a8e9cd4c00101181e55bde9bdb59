package sec02_exam05_float_duble;

public class JavaLangObject {

	public static void main(String[] args) {
		// 실수형 데이터 타입: float(4), double(8) 
		
		double var1 = 3.14; //3.14 b가 생략 
		//float var2 =3.14; double형 데이터탑입 8바이트로 인식하기 때문에 대입에러 발생
		float var3 = 3.14F; //float 실수값은 접미사로 f or F를 
		
		//정밀도 테스트 
		double var4 = 0.123456790123456789; //소수 19자리,콘솔을 보면 9가 반올림 돤걸 알수 있다
		
		float var5 = 0.123456790123456789F; //float은 
		
		System.out.println(var1);
		System.out.println(var3); 
		System.out.println(var4); 
		System.out.println(var5);
	}

}
