package sec04.exam07_conditinal;

public class ConditionalOperatorExample02 {

	public static void main(String[] args) {
		// 삼항 연산자 조건 int x = 10 이고 
		//int y = 20; 
		// int z (++x) + (y--);
      
		
		int x =10; 
		int y =20; 
		int z = (++x) + (y--); 
		
		System.out.println("x= " , x);  
		System.out.println("y=", y); 
		System.out.println();
	}

}
