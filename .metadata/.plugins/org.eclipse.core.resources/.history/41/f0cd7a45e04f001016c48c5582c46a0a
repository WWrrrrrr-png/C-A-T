package esc06.exam01_array_byltst;

public class ArrayCreateByValueListExample1 {

	public static void main(String[] args) {
		// 참조형 데이터 타입 배열: 동일한 데이터 타입으로 구성하여 , 연속적으로 이루어진 기억장소
				
				//기본 데이터 타입,영역 : 스택영역에 데이터를 저장
 				int score1= 83;
				int score2= 90; 
				int score3= 87; 
				
				int space= "-------";
				System.out.println(space);
				//참조타입 영역: 스택=주소,힙=데이터 저장
				int[] scores = {83,90,87};
				
		
		
		
		System.out.println(score1); 
		System.out.println(score2); 
		System.out.println(score3); 
		
		System.out.println(scores[0]);
		System.out.println(scores[1]); 
		System.out.println(scores[2]); 
		System.out.println("3개의 합은?= "+ (score1 +score2+score3 )); 
		
		int sum=0; 
		for(i=0; i<3; i++) {
			sum += scores[i];
		}
	System.out.println("3개의 합은 ?=" +sum);
	
	}

}
