package sec03.exam01_arthmertic;

public abstract sealed class StringEqualExample {

	public static void main(String[] args) {
		// 문자열 비교예제 
		
		String strVar1 = "난 김카타리나"; 
		String strVar2 = "난 김카타리나"; 
		String strVar3 = new String("난 김카타리나"); 
		
		System.out.println(strVar1 == strVar2);  
		System.out.println(strVar1 == strVar3); 
		System.out.println();

	}

}
