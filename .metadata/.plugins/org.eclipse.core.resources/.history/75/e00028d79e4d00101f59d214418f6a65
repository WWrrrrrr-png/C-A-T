package sec04.exam01_if;

public class ForSumFromExample {

    public static void main(String[] args) {
      //whlie문으로 1~100까지 누적된 합 구하기 
    	
    	int  sum =0; 
    	int i = 1; 
    	
    	while(i<=100) {
    		sum += i;
    		
    		i++; 
    		
    		
    	
    		
    	}
    System.out.println("1~100까지 누적된 값은?"+ sum);

    
    
    }}



