package sec03.exam02_promotion;

public class CastingExample {

	public static void main(String[] args) {
		// casting 명시적 형변환은 강제적 형변환 기존의 바이트 유지x
        // samll 데이터 타입 = big데이터 타입
	   
		int intValue = 44032; 
		char charValue = (char) intValue; //자바에서는 큰 데이터 타입은 작은 테이더 타입x 당연한거 결국 값은 손실
		                 //(char) 마우스 드래그 add 
		System.out.println(charValue);
	}

}
