package sec03_exam_long;

public class LongExample {

	public static void main(String[] args) {
		// long 데이터타입 (8바이트) 
		// 숫자값을 사용시 기본적으로 int로 해석한다
      
		long var1 = 10; //int 우측이 의값이 좌측이 값보다 작을시 다른 데이터타입 사용가능
		long var2 = 20L; //long 데이터 타입은 변수값뒤에 대문자 L 
		
		//long ver3 = 1000000000000; //int형 범위를 벗어났다 1빼놓고 0 12개 오버플로우가 되서 long 으로 표현
		long var4 = 10000000000L; 
		System.out.println(var1); 
		System.out.println(var2); 
		System.out.println(var4); 
		
	}

}
