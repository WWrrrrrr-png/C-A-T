package esc03exam04_break;

public class BreackOutterExample {

	public static void main(String[] args) {
		//A=65 Z=96   
		
		for(char upper = 'A'; upper <= 'z'; upper++) {
			for(char lower ='a'; lower <= 'z'; lower++) {
				System.out.print(upper + "-"+lower+"\t");
			}
			System.out.println();
		}

	}

}
