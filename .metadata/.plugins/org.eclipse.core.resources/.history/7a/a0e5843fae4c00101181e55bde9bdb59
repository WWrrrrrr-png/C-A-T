package sec03.exam02_promotion;

public class PromotionExample {

	public static void main(String[] args) {
		// promotion 묵시적 형변환 
	 byte byteValue =10; 
	 int intValue = byteValue; //int intValue =(int) byteValue;
	 
	 System.out.println(intValue); 
	 char charValue = '카'; 
	 intValue= charValue;
	}

}
